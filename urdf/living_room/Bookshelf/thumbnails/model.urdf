<robot name="Bookshelf">
  <link name="link">
    <inertial>
      <mass value="1.0"/>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1"/>
    </inertial>

    <!-- Back collision and visual -->
    <collision name="back">
      <origin xyz="0 0.005 0.6" rpy="0 0 0"/>
      <geometry>
        <box size="0.9 0.01 1.2"/>
      </geometry>
    </collision>
    <visual name="visual1">
      <origin xyz="0 0.005 0.6" rpy="0 0 0"/>
      <geometry>
        <box size="0.9 0.01 1.2"/>
      </geometry>
      <material name="wood">
        <texture filename="bookshelf_diffuse.jpg"/>
      </material>
    </visual>

    <!-- Left side collision and visual -->
    <collision name="left_side">
      <origin xyz="0.45 -0.195 0.6" rpy="0 0 0"/>
